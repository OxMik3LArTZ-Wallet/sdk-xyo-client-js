{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/node-core-lib",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.73.3",
  "description": "XYO Archivist",
  "sideeffects": false,
  "engines": {
    "node": ">=16 <17"
  },
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./dist/docs.json": {
      "default": "./dist/docs.json"
    },
    "./cjs": {
      "default": "./dist/index.js"
    },
    "./docs": {
      "default": "./dist/docs.json"
    },
    "./esm": {
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "author": "Arie Trouw",
  "license": "LGPL-3.0",
  "scripts": {
    "package-compile": "tsup && publint",
    "package-recompile": "tsup && publint"
  },
  "dependencies": {
    "@xyo-network/boundwitness-model": "workspace:~",
    "@xyo-network/node-core-model": "workspace:~",
    "@xyo-network/payload-model": "workspace:~",
    "@xyo-network/payload-wrapper": "workspace:~",
    "@xyo-network/schema-cache": "workspace:~",
    "@xyo-network/schema-payload-plugin": "workspace:~",
    "ajv": "^8.12.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/source-map-support": "0.5.7",
    "@xylabs/ts-scripts-yarn3": "^2.19.8",
    "@xylabs/tsconfig": "^2.19.8",
    "publint": "^0.2.2",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
