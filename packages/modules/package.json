{
  "name": "@xyo-network/modules",
  "author": {
    "email": "support@xyo.network",
    "name": "XYO Development Team",
    "url": "https://xyo.network"
  },
  "bugs": {
    "email": "support@xyo.network",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-client-js/issues"
  },
  "dependencies": {
    "@xyo-network/archivist": "workspace:~",
    "@xyo-network/bridge": "workspace:~",
    "@xyo-network/diviner": "workspace:~",
    "@xyo-network/module": "workspace:~",
    "@xyo-network/node": "workspace:~",
    "@xyo-network/sentinel": "workspace:~",
    "@xyo-network/witness": "workspace:~"
  },
  "devDependencies": {
    "@swc/core": "^1.3.86",
    "@xylabs/ts-scripts-yarn3": "^3.0.27",
    "@xylabs/tsconfig": "^3.0.27",
    "typescript": "^5.2.2"
  },
  "description": "Primary SDK for using XYO Protocol 2.0",
  "docs": "dist/docs.json",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./dist/docs.json": {
      "default": "./dist/docs.json"
    },
    "./cjs": {
      "default": "./dist/index.js"
    },
    "./docs": {
      "default": "./dist/docs.json"
    },
    "./esm": {
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "homepage": "https://xyo.network",
  "license": "LGPL-3.0",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-client-js.git"
  },
  "sideEffects": false,
  "version": "2.74.0"
}
